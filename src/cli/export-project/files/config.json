{
  "command": "export:project",
  "name": "export:project",
  "aliases": [
    "exp:p"
  ],
  "usage": "[options]",
  "options": [
    {
      "keys": "-h, --help",
      "description": "Help for Exporting project code"
    }
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "basePathForPages": "./app/project/components/pages/",
  "temporaryFilePath": "./../dist/",
  "zipFileDestinationPath": "./dist/",
  "ignoreFilesFolders": [
    "node_modules",
    "dist",
    "www",
    "/out-tsc",
    ".project",
    ".classpath",
    ".settings/",
    ".sublime-workspace",
    ".sass-cache",
    "npm-debug.log",
    "yarn-error.log",
    "testem.log",
    ".DS_Store",
    ".lock",
    "package-lock.json",
    "Thumbs.db"
  ]
}