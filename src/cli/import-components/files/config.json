{
  "command": "import:components",
  "name": "import:components",
  "aliases": [
    "imp:c"
  ],
  "usage": "[options]",
  "options": [
    {
      "keys": "-h, --help",
      "description": "Help for Importing components from the './app/project/components/' folder"
    },
    {
      "keys": "-z, --zipFile",
      "description": "Prefix for full location path of the zip file to be imported"
    },
{
      "keys": "-l, --list",
      "description": "Prefix for list of file names"
    },
    {
      "keys": "-f, --force",
      "description": "Force the program to overwrite the target files with the source files and avoid any confirmation"
    },
    {
      "keys": "--all",
      "description": "Select this option to import all the files"
    }
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "basePathForPages": "./app/project/components/pages/",
  "requiredFolderPath": "./app/project/components/",
  "zipFolderName": "imported-components",
  "exportedFolderName": "exported-components",
  "outputFileName": "imported-components.zip",
  "outputTempFolderName": "Imported-Components-Code-Folder-Temp",
  "zipFileDestinationPath": "./dist/",
  "ignoreFilesFolders": [
    ".DS_Store",
    ".lock",
    "Thumbs.db"
  ]
}