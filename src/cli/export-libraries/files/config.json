{
  "command": "export:library",
  "name": "export:library",
  "usage": "[options]",
  "options": [
    {
      "keys": "-l, --list",
      "description": "Prefix for list of library file names"
    },
    {
      "keys": "--all",
      "description": "Select this option to export all the library files"
    }  
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "requiredFolderPath": "./app/project/libraries/",
  "zipFolderName": "exported-library",
  "outputFileName": "exported-library.zip",
  "outputTempFolderName": "Exported-Library-Code-Folder-Temp",
  "zipFileDestinationPath": "./dist/",
  "ignoreFilesFolders": [
    ".DS_Store",
    ".lock",
    ".db"
  ]
}