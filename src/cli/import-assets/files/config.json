{
  "command": "delete:components",
  "name": "delete:components",
  "usage": "[options]",
  "options": [
    {
      "keys": "-z, --zipFile",
      "description": "Prefix for full location path of the zip file to be imported"
    },
{
      "keys": "-l, --list",
      "description": "Prefix for list of component names to be deleted"
    },
    {
      "keys": "-f, --force",
      "description": "Forces the component to be deleted without asking for a confirmation"
    }
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "requiredFolderPath": "./app/project/assets/",
  "zipFolderName": "imported-assets",
  "exportedFolderName": "exported-assets",
  "outputFileName": "imported-assets.zip",
  "outputTempFolderName": "Imported-Assets-Code-Folder-Temp",
  "ignoreFilesFolders": [
    ".DS_Store",
    ".lock",
    "Thumbs.db"
  ],
  "zipFileDestinationPath": "./dist/"
}