{
  "command": "delete:components",
  "name": "delete:components",
  "usage": "[options]",
  "options": [
    {
      "keys": "-l, --list",
      "description": "Prefix for list of component names to be deleted"
    },
    {
      "keys": "-f, --force",
      "description": "Forces the component to be deleted without asking for a confirmation"
    }
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "basePathForPages": "./app/project/components/pages/",
  "requiredFolderPath": "./app/project/components/",
  "zipFolderName": "exported-components",
  "outputFileName": "exported-components.zip",
  "outputTempFolderName": "Exported-Components-Code-Folder-Temp",
  "zipFileDestinationPath": "./dist/",
  "ignoreFilesFolders": [
    ".DS_Store",
    ".lock",
    ".db"
  ]
}