{
  "command": "export:assets",
  "name": "export:assets",
  "usage": "[options]",
  "options": [
    {
      "keys": "-l, --list",
      "description": "Prefix for list of asset file names"
    },
    {
      "keys": "--all",
      "description": "Select this option to export all the assets"
    }
  ],
  "additionalHelp": true,
  "logger": {
    "allowLogging": true,
    "logLevel": 1
  },
  "requiredFolderPath": "./app/project/assets/",
    "zipFolderName": "exported-assets",
    "outputFileName": "exported-assets.zip",
    "outputTempFolderName": "Exported-Assets-Code-Folder-Temp",
    "zipFileDestinationPath": "./dist/",
    "ignoreFilesFolders": [
      ".DS_Store",
      ".lock",
      ".db"
    ]
}