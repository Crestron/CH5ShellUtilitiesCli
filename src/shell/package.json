{
  "name": "shell-template",
  "version": "2.0.0-ve.07",
  "main": "index.js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "clean": "rimraf dist",
    "cleanjs": "rimraf dist/Shell/*.js",
    "cleanjs:start": "rimraf dist/dev/Shell/*.js",
    "cleanjs:prod": "rimraf dist/prod/Shell/*.js",
    "clean:start": "rimraf dist/dev",
    "clean:prod": "rimraf dist/prod",
    "start": "npm run val:pc && npm run clean:start && webpack --config webpack.dev.js",
    "build:prod": "npm run val:pc && npm run clean:prod && webpack --config webpack.prod.js && npm run cleanjs:prod",
    "build:archive": "npm run build:prod && ch5-cli archive -P samplesource=Shell -d dist/prod/Shell -o dist/prod -c config/contract.cse2j",
    "build:deploy": "ch5-cli deploy -H 192.168.0.103 -t touchscreen dist/prod/shell-template.ch5z",
    "build:deploywithpassword": "ch5-cli deploy -H 192.168.0.103 -t touchscreen dist/prod/shell-template.ch5z",
    "build:onestep": "npm run build:archive && npm run build:deploy",
    "build:onestepwithpassword": "npm run build:archive && npm run build:deploywithpassword",
    "delete:components": "ch5-shell-cli delete:components",
    "del:c": "ch5-shell-cli del:c",
    "export:all": "ch5-shell-cli export:all",
    "export:assets": "ch5-shell-cli export:assets",
    "exp:a": "ch5-shell-cli exp:a",
    "export:components": "ch5-shell-cli export:components",
    "exp:c": "ch5-shell-cli exp:c",
    "export:library": "ch5-shell-cli export:library",
    "exp:l": "ch5-shell-cli exp:l",
    "export:project": "ch5-shell-cli export:project",
    "exp:p": "ch5-shell-cli exp:p",
    "generate:page": "ch5-shell-cli generate:page",
    "gen:p": "ch5-shell-cli gen:p",
    "generate:widget": "ch5-shell-cli generate:widget",
    "gen:w": "ch5-shell-cli gen:w",
    "import:all": "ch5-shell-cli import:all",
    "import:assets": "ch5-shell-cli import:assets",
    "imp:a": "ch5-shell-cli imp:a",
    "import:components": "ch5-shell-cli import:components",
    "imp:c": "ch5-shell-cli imp:c",
    "import:library": "ch5-shell-cli import:library",
    "imp:l": "ch5-shell-cli imp:l",
    "validate:projectconfig": "ch5-shell-cli validate:project-config",
    "val:pc": "ch5-shell-cli val:pc",
    "update:project": "ch5-shell-cli update:project"
  },
  "devDependencies": {
    "@crestron/ch5-shell-utilities-cli": "https://siproducts.blob.core.windows.net/ch5-test/100421/CH5ShellUtilitiesCli-2.0.2-alpha.2.tgz",
    "@crestron/ch5-utilities-cli": "^0.1.52",
    "browser-sync": "^2.27.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.0.4",
    "create-file-webpack": "^1.0.2",
    "css-loader": "^3.0.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.5.2",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.7.0",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^7.1.0",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-merge": "^4.2.1",
    "write-json-webpack-plugin": "^1.1.0"
  },
  "dependencies": {
    "@crestron/ch5-crcomlib": "https://siproducts.blob.core.windows.net/ch5-test/121721/CH5CrComLib-2.0.10-alpha.10.tgz",
    "@crestron/ch5-theme": "https://siproducts.blob.core.windows.net/ch5-test/121721/CH5Themes-2.0.8-alpha.8.tgz",
    "@crestron/ch5-webxpanel": "file:crestron-ch5-webxpanel-2.0.0.tgz"
  },
  "files": [
    "copyright.txt",
    "LICENSE.txt",
    "*.js",
    "README.md",
    "app/**/*",
    "config/**/*",
    ".vscode/**/*",
    "package-lock.json",
    "yarn.lock"
  ]
}
